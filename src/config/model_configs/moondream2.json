{
  "model_name": "Moondream2",
  "model_id": "moondream2",
  "model_path": "vikhyatk/moondream2",
  "device": "mps",
  "timeout": 60,
  "max_tokens": 100,
  "version": "standard",
  "description": "Standard Moondream2 vision language model",
  
  "capabilities": {
    "vision": true,
    "text_generation": true,
    "image_understanding": true,
    "conversation": true
  },
  
  "model_config": {
    "torch_dtype": "float32",
    "device_map": null,
    "trust_remote_code": true,
    "low_cpu_mem_usage": false
  },
  
  "image_processing": {
    "size": [384, 384],
    "max_size": 512,
    "format": "RGB",
    "quality": 95,
    "preserve_aspect_ratio": true
  },
  
  "generation_config": {
    "max_new_tokens": 100,
    "do_sample": false,
    "temperature": 0.7,
    "top_p": 0.9,
    "repetition_penalty": 1.1
  },
  
  "server": {
    "type": "fastapi",
    "host": "0.0.0.0",
    "port": 8080,
    "cors_enabled": true,
    "log_level": "info"
  },
  
  "memory": {
    "cleanup_interval": 10,
    "aggressive_cleanup": false,
    "required_gb": 4.0
  },
  
  "api": {
    "openai_compatible": true,
    "endpoints": [
      "/",
      "/health", 
      "/v1/chat/completions"
    ]
  },
  
  "performance": {
    "optimization_level": "none",
    "precision": "float32",
    "caching": false,
    "batch_processing": false
  },
  
  "ui": {
    "default_instruction": "You are an AI visual observer. Analyze this image and provide:\n\n**OBJECTS**: What objects, tools, or items are visible?\n**PEOPLE**: Are there any people? What are they doing?\n**ACTIONS**: What activities or movements are occurring?\n**ENVIRONMENT**: What type of place or setting is this?\n**NOTABLE**: What catches your attention or seems important?\n\nFocus on describing what you actually see in this image. Be specific and observant.",
    "capture_intervals": [1000, 2000, 5000, 10000],  
    "default_interval": 5000
  }
}