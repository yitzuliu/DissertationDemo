{
  "model_name": "YOLO8",
  "model_id": "yolo8",
  "model_path": "yolov8n.pt",
  "device": "auto",
  "timeout": 30,
  "max_tokens": 50,
  "version": "1.0.0",
  "description": "YOLO8 object detection model for real-time object detection and tracking",
  
  "capabilities": {
    "object_detection": true,
    "tracking": true,
    "segmentation": false,
    "classification": false
  },
  
  "model_config": {
    "confidence": 0.25,
    "iou_threshold": 0.45,
    "max_detections": 300,
    "agnostic_nms": false
  },
  
  "image_processing": {
    "size": [640, 640],
    "format": "RGB",
    "normalize": true,
    "letterbox": true
  },
  
  "server": {
    "host": "0.0.0.0",
    "port": 8080,
    "framework": "fastapi"
  },
  
  "ui": {
    "default_instruction": "Detect and identify all objects in this image.",
    "capture_intervals": [500, 1000, 2000, 5000],
    "default_interval": 1000
  }
}
