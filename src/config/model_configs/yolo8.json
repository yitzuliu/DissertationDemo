{
  "model_name": "YOLO8",
  "model_path": "models/yolo8/yolov8n.pt",
  "server": {
    "command": "python",
    "args": ["models/yolo8/run_yolo.py"],
    "port": 8081,
    "health_endpoint": "/health"
  },
  "image_processing": {
    "size": [640, 640],
    "format": "jpeg",
    "jpeg_quality": 90
  },
  "detection": {
    "confidence_threshold": 0.5,
    "iou_threshold": 0.7,
    "max_detections": 100
  },
  "api": {
    "endpoint": "/v1/detect",
    "timeout": 30,
    "response_format": "json"
  },
  "ui": {
    "default_instruction": "Detect and identify all objects in this image.",
    "capture_intervals": [1000, 2000, 5000],
    "default_interval": 2000,
    "show_bboxes": true,
    "show_confidence": true
  },
  "version": "1.0.0",
  "description": "YOLO8 object detection model configuration"
}
